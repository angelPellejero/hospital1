<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Medico" table="Medico">
    <id name="idmedico" type="integer" column="IdMedico">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ncolegiado" type="integer" column="Ncolegiado" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nombre" type="string" column="Nombre" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellido" type="string" column="Apellido" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="Telefono" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
